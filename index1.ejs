<%
var dt = new Date();
var y = dt.getFullYear();
var m = dt.getMonth();
var d = dt.getDay();
%>
<!DOCTYPE html>
<html>
    <head>
        <meta charset='UTF-8'>
        <title><%=title %></title>
        <link type="text/css" href="./style.css" rel="stylesheet">
    </head>
    <body>
        <h1><%=title %></h1>
        <b1><%=msg %></b1>
        <p>今日は、<%= y + '年' + (m + 1) + '月' + (d+1) + '日'%>です</p>
        <table>
        <% for(var i =0; i < datas.length; i++){%>
        <tr>
        <td><%= datas[i].name %></td>
        <td><%= datas[i].mail %></td>
        <td><%= datas[i].tel %></td>
        </tr>
        <% }%>
        </table>
    </body>
</html>